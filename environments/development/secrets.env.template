# Environment Variables for Development Environment
# Copy this file to .env and fill in your actual values
# NEVER commit actual secrets to version control!

# Bot Credentials
ROSEY_BOT_PASSWORD_DEV=your_bot_password_here
ROSEY_CHANNEL_PASSWORD_DEV=your_channel_password_here

# API Keys
OPENAI_API_KEY_DEV=sk-your_openai_api_key_here
OPENWEATHER_API_KEY_DEV=your_openweather_api_key_here

# Database (if using PostgreSQL)
ROSEY_DB_PASSWORD_DEV=your_database_password_here

# Admin Credentials (for Web UI)
ROSEY_ADMIN_USER_DEV=admin
ROSEY_ADMIN_PASSWORD_DEV=your_admin_password_here

# API Authentication
ROSEY_API_TOKEN_DEV=your_api_token_here

# Optional: External Services
# SENTRY_DSN=your_sentry_dsn_here
# DATADOG_API_KEY=your_datadog_key_here
