# Environment Variables for Staging Environment
# Copy this file to .env and fill in your actual values
# NEVER commit actual secrets to version control!

# Bot Credentials
ROSEY_BOT_PASSWORD_STAGING=your_bot_password_here
ROSEY_CHANNEL_PASSWORD_STAGING=your_channel_password_here

# API Keys
OPENAI_API_KEY_STAGING=sk-your_openai_api_key_here
OPENWEATHER_API_KEY_STAGING=your_openweather_api_key_here

# Database
ROSEY_DB_PASSWORD_STAGING=your_database_password_here

# Admin Credentials (for Web UI)
ROSEY_ADMIN_USER_STAGING=admin
ROSEY_ADMIN_PASSWORD_STAGING=your_admin_password_here

# API Authentication
ROSEY_API_TOKEN_STAGING=your_api_token_here

# Optional: External Services
# SENTRY_DSN=your_sentry_dsn_here
# DATADOG_API_KEY=your_datadog_key_here
